	pages = {988--995},
	file = {Mardones et al. - 2017 - Toxic dinoflagellate blooms of Alexandrium catenel.pdf:/home/rfvera/Zotero/storage/9B9EGKQF/Mardones et al. - 2017 - Toxic dinoflagellate blooms of Alexandrium catenel.pdf:application/pdf},
