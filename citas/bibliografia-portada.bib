	keywords = {climate change, analyse des tendances, Andes, changement climatique, detection and attribution, détection et attribution, événements extrêmes, extreme events, hydroclimatologie, hydroclimatology, land use, trend analysis, utilisation des sols},
	pages = {1598--1619},
