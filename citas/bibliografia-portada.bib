@article{baylon_coritoma_evaluacion_2018,
	title = {Evaluación de la diversidad de algas fitoplanctónicas como indicadores de la calidad del agua en lagunas altoandinas del departamento de {Pasco} ({Perú})},
	volume = {17},
	issn = {1993-9507, 1726-2216},
	url = {http://revistas.lamolina.edu.pe/index.php/eau/article/view/1180},
	doi = {10.21704/rea.v17i1.1180},
	abstract = {In the Department of Pasco,Peru, mining activity has been the main economic activity for decades, having generated environmental pollutants such as mine tailings, acidic waters and smoke emissions from mines, concentration plants and foundries as well as negative impacts to high Andean lakes in the upper basins. The aim of this study was to evaluate the water quality of five high andean lakes (Lacsacocha, Yanamate, Quiulacocha, Milpo Andina and Huaroncocha) in the department of Pasco, based on phytoplankton abundance, richness and diversity. For the collection of samples, 40 l of water were filtered on the shore, using a phytoplankton net of 10 μm. Four parameters were evaluated: pH, conductivity, temperature and concentration of metals in the water. The biotic indexes used for water quality assessment were the Shannon-Wiener diversity (H ') and the general diatomic index (GDI). 88 taxa were registered, distributed in seven phyla: Bacillariophyta (28), Charophyta (11), Chlorophyta (30), Cyanobacteria (15), Euglenophyta (2), Ochrophyta (1) and Miozoa (1). The most frequent phylum was Bacillariophyta (69.16\%), followed by Chlorophyta (24.26\%). Biotic indexes indicated that Yanamate, Lacsacocha, Quiulacocha and Milpo Andina were polluted while Huaroncocha waters had optimal biological quality.},
	language = {es},
	number = {1},
	urldate = {2023-08-10},
	journal = {Ecología Aplicada},
	author = {Baylón Coritoma, Maribel and Roa Castro, Karen and Libio Sánchez, Tito and Tapia Ugaz, Liliana and Jara Pena, Enoc and Macedo Prada, Diego and Salvatierra Sevillano, Alan and Dextre Rubina, Abigail},
	month = jul,
	year = {2018},
	pages = {119},
	file = {Baylón Coritoma et al. - 2018 - Evaluación de la diversidad de algas fitoplanctóni.pdf:/home/rfvera/Zotero/storage/MQR5PHBA/Baylón Coritoma et al. - 2018 - Evaluación de la diversidad de algas fitoplanctóni.pdf:application/pdf},
