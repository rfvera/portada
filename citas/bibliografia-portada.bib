@article{mariani_effects_2015,
	title = {Effects of trophic status on microcystin production and the dominance of cyanobacteria in the phytoplankton assemblage of {Mediterranean} reservoirs},
	volume = {5},
	issn = {2045-2322},
	url = {https://www.nature.com/articles/srep17964},
	doi = {10.1038/srep17964},
	abstract = {Abstract
            
              The aim of our study was to evaluate the abundance of cyanobacteria and microcystins in four Sardinian reservoirs (Italy) characterised by different trophic status to define a reference picture for future changes. Increasing levels of eutrophication and the abundance of cyanobacteria are expected to occur due to climate change, especially in the southern Mediterranean. Consequently, an in-depth study of the occurrence of harmful cyanobacteria is important to develop appropriate management strategies for water resources at a local scale. Monthly samples were collected at one station in each reservoir over an 18-month period. The Analysis of similarity indicated that cyanobacterial abundance and species composition differed significantly among the reservoirs. The Redundancy analysis highlighted their relationship to trophic, hydrological and seasonal patterns. Spearman’s analysis indicated that there were significant correlations among the most important species (
              Planktothrix agardhii–rubescens
              group,
              Aphanizomenon flos-aquae
              and
              Dolichospermum planctonicum
              ), nutrients and microcystins. We highlighted that the species composition during periods of maximum microcystin concentrations differed from those typically reported for other Mediterranean sites. We found new potential microcystin producers (
              Aphanizomenon klebahnii
              ,
              Dolichospermum macrosporum
              and
              Dolichospermum viguieri
              ), which emphasised the high diversity of cyanobacteria in the Mediterranean area and the need for detailed research at the local scale.},
	language = {en},
	number = {1},
	urldate = {2023-08-10},
	journal = {Scientific Reports},
	author = {Mariani, Maria Antonietta and Padedda, Bachisio Mario and Kaštovský, Jan and Buscarinu, Paola and Sechi, Nicola and Virdis, Tomasa and Lugliè, Antonella},
	month = dec,
	year = {2015},
	pages = {17964},
	file = {Informe Final Lagos_X etapa _2022-2023 _ web.pdf:/home/rfvera/Zotero/storage/77GNQBX2/Informe Final Lagos_X etapa _2022-2023 _ web.pdf:application/pdf;Mariani et al. - 2015 - Effects of trophic status on microcystin productio.pdf:/home/rfvera/Zotero/storage/BM3ND67R/Mariani et al. - 2015 - Effects of trophic status on microcystin productio.pdf:application/pdf},
