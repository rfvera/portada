@article{ake_castillo_calculation_2016,
	title = {Calculation of the cell carbon content of four taxa of diatom: biovolume and spectrophotometry},
	volume = {26},
	issn = {0188-8897, 2448-7333},
	shorttitle = {Calculation of the cell carbon content of four taxa of diatom},
	url = {http://hidrobiologica.izt.uam.mx/index.php/revHidro/article/view/501},
	doi = {10.24275/uam/izt/dcbs/hidro/2016v26n2/Ake},
	abstract = {Background. Phytoplankton biomass is an essential parameter in aquatic ecosystems; two of the most widely used indicators for estimating phytoplankton biomass are chlorophyll-a (Chl-a) by spectrophotometry and biovolume (BV). Goals. To show how different results are when estimating phytoplankton biomass by means of the spectrophotometric and BV methods. Methods. Biomass by Chl-a and BV were estimated in four cultures of marine diatoms. For estimating BV, we used live cells and acid Lugol’s solution fixed cells. Results. Biomass values were noticeably different: the BV for Thalassiosira hispida and Skeletonema costatum underestimates ≈41\% of the biomass with respect to values obtained by spectrophotometry. In contrast, the BV for Pseudo-nitzschia sp. and Cylindrotheca closterium overestimates the biomass by 4\% and 25\%, respectively. In the case of estimated biovolume, the results indicate that fixer acid Lugol’s solution modified the size of the cell significantly in C. closterium and Pseudo-nitzschia sp. (p{\textless}0.05), but not for S. costatum and T. hispida (p{\textgreater}0.05). Two equations are provided for biomass calculation when studying live (pgC/cell = -1.5567 + 0.1428 (BV)) or acid Lugol’s solution fixed (pgC/cell = -5.0126 + 0.1644 (BV)) diatom cells. Conclusions. Although BV is useful, it can have drawbacks because it produces values above and below those estimated by spectrophotometry, in addition to the fact that acid Lugol´s solution modifies cell size. We would advise considering the available information based in Chl-a or carbon of different species.},
	language = {es},
	number = {2},
	urldate = {2023-08-10},
	journal = {Hidrobiológica},
	author = {Aké Castillo, José Antolín and {Universidad Veracruzana. Instituto de Ciencias Marinas y Pesquerías. Boca del Río, Veracruz, México}},
	month = aug,
	year = {2016},
	pages = {251--257},
	file = {Aké Castillo y Universidad Veracruzana. Instituto de Ciencias Marinas y Pesquerías. Boca del Río, Veracruz, México - 2016 - Calculation of the cell carbon content of four tax.pdf:/home/rfvera/Zotero/storage/SV6TQCIS/Aké Castillo y Universidad Veracruzana. Instituto de Ciencias Marinas y Pesquerías. Boca del Río, Veracruz, México - 2016 - Calculation of the cell carbon content of four tax.pdf:application/pdf},
}

